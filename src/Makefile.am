manaplus_SOURCES += manaplus.rc
dyecmd_SOURCES += manaplus.rc
endif

if ENABLE_CILKPLUS
dyecmd_CXXFLAGS += -DENABLE_CILKPLUS -fcilkplus -lcilkrts
