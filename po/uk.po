#. TRANSLATORS: command line help
#: src/commandline.cpp:56
msgid "Options:"
msgstr "Параметри:"

#. TRANSLATORS: command line help
#: src/commandline.cpp:59
msgid "  -l --log-file       : Log file to use"
msgstr "  - --log-file        : Лог-файл"

